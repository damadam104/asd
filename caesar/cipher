#include <cs50.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int main (int argc,string argv[])
{ 
    if (argc != 2)
    {
      printf(" Usage: ./asciimath key");
      return 1;
    }
    //// key is second command argument
    int key = atoi(argv[1]);
    int x=0;
    string sMess=GetString();
    if (sMess[x]>'A'&& sMess[x]<='Z')
    { sMess[x]=(sMess[x]-'A'+key)%26+'A';}
     for (x=0;x<strlen(sMess);x++)
    
     if (sMess[x]>'a'&&sMess[x]<'z')
    {sMess[x]=(sMess[x]-'a'+key)%26+'a';
    }
    for (x=0;x<strlen(sMess);x++)
   {printf("%c", sMess[x]);
   
  
    } 
   }
